library "m"
link "float.c"

export bool = foreign "rain_ext_truthy"(self)
export float = func(self) -> self
export int = foreign "rain_ext_float_int"(self)
export str = foreign "rain_ext_float_str"(self)

export floor = foreign "rain_ext_float_floor"(self)
export ceil = foreign "rain_ext_float_ceil"(self)
export round = foreign "rain_ext_float_round"(self)
export abs = foreign "rain_ext_float_abs"(self)
