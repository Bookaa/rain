macro print_all(args) as (args)
  let node = ast.block:empty()
  save node

  for arg in ast.list.items(args)
    let call = ast.call:init(ast.name:init("print"), table)
    call.args[0] = arg
    node:add(call)

macro call(expr, argblock) as (fn, args)
  return ast.call:init(fn, args)

macro catchcall(expr, argblock) as (fn, args)
  return ast.catchcall:init(fn, args)
