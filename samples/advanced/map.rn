import "types/array"
import iter

let double = func(n) -> n * 2

let main = func()
  let a = table :: array
  a[0] = 10
  a[1] = 11
  a[2] = 12
  a[3] = 13

  print("keys from a:")
  for x in a:keys()
    print(x)

  print("values from a:")
  for x in a:values()
    print(x)

  print("doubled values from a:")
  for x in iter.map(a:values(), double)
    print(x)

  let b = a:map(double)

  print("key/values from b:")
  for k, v in b:keys(), b:values()
    print(tostr(k) $ " = " $ tostr(v))
