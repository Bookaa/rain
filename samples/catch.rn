import string

let three = func()
  panic("three!")

let two = func()
  let x = three?()
  print("two caught " $ string.tostr(x))

let one = func()
  catch x
    two()
  print("one caught " $ string.tostr(x))

let main = func()
  print("----")
  catch x
    print("before")
    one()
    print("after")
  print("main caught " $ string.tostr(x))

  print("----")
  print("before")
  x = one?()
  print("after")
  print("main caught " $ string.tostr(x))

  print("----")
  catch x
    print("before")
    panic("obvious")
    print("after")
  print("main caught " $ string.tostr(x))

  print("----")
  print("before")
  x = panic?("obvious")
  print("after")
  print("main caught " $ string.tostr(x))
